<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="data.ExcluirProdutorController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Arquivo">
          <items>
                  <MenuItem mnemonicParsing="false" text="Armazenagem Existentes" />
                  <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" onAction="#listarProdutor" onMenuValidation="#listarProdutor" text="Listar Produtores" />
                  <MenuItem mnemonicParsing="false" onAction="#alterarProdutor" onMenuValidation="#alterarProdutor" text="Alterar Produtor" />
                  <MenuItem disable="true" mnemonicParsing="false" onAction="#excluirProdutor" onMenuValidation="#excluirProdutor" text="Excluir Produtor" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" onAction="#voltar" onMenuValidation="#voltar" text="Voltar ao Menu Principal" />
            <MenuItem mnemonicParsing="false" onAction="#fechar" onMenuValidation="#fechar" text="Sair">
                     <accelerator>
                        <KeyCodeCombination alt="DOWN" code="F4" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Ajuda">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#ajuda" text="Ajuda">
                     <accelerator>
                        <KeyCodeCombination alt="DOWN" code="F1" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Pane layoutY="112.0" opacity="0.9" prefHeight="577.0" prefWidth="1367.0" style="-fx-background-color: #fff; -fx-border-color: #bbb;">
               <effect>
                  <Lighting>
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     <light>
                        <Light.Distant />
                     </light>
                  </Lighting>
               </effect>
               <children>
                  <Button cancelButton="true" layoutX="690.0" layoutY="459.0" mnemonicParsing="false" onAction="#excluirProdutor" prefHeight="90.0" prefWidth="180.0" stylesheets="@../styles/Style.css" text="Excluir" textAlignment="CENTER">
                     <font>
                        <Font name="Times New Roman" size="24.0" />
                     </font>
                     <styleClass>
                        <String fx:value="button" />
                        <String fx:value="button" />
                     </styleClass>
                  </Button>
                  <Button layoutX="497.0" layoutY="459.0" mnemonicParsing="false" onAction="#limparCampos" prefHeight="90.0" prefWidth="180.0" stylesheets="@../styles/Style.css" text="Limpar" textAlignment="CENTER">
                     <font>
                        <Font name="Times New Roman" size="24.0" />
                     </font>
                  </Button>
                  <Label layoutX="613.0" layoutY="369.0" text="ID:" textAlignment="CENTER">
                     <font>
                        <Font name="Times New Roman" size="48.0" />
                     </font>
                  </Label>
                  <TextField fx:id="campoID_" layoutX="690.0" layoutY="376.0" prefHeight="40.0" prefWidth="180.0">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </TextField>
                  <Label alignment="CENTER" layoutX="8.0" layoutY="14.0" opacity="0.4" prefHeight="125.0" prefWidth="1339.0" style="-fx-alignment: center;" text="Excluir produtor" wrapText="true">
                     <font>
                        <Font name="Times New Roman" size="90.0" />
                     </font>
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Label>
                  <Text layoutX="571.0" layoutY="446.0" opacity="0.2" strokeType="OUTSIDE" strokeWidth="0.0" text="..." textAlignment="CENTER" wrappingWidth="226.0">
                     <font>
                        <Font name="Times New Roman" size="18.0" />
                     </font>
                  </Text>
                  <Button layoutX="615.0" layoutY="311.0" mnemonicParsing="false" onAction="#listagemProdutores" prefHeight="44.0" prefWidth="166.0" stylesheets="@../styles/Style.css" text="Listar" textAlignment="CENTER">
                     <font>
                        <Font name="Times New Roman" size="24.0" />
                     </font>
                  </Button>
                  <TextArea fx:id="inputList" focusTraversable="false" layoutX="371.0" layoutY="139.0" prefHeight="163.0" prefWidth="654.0" wrapText="true">
                     <font>
                        <Font name="Times New Roman" size="14.0" />
                     </font>
                  </TextArea>
               </children>
            </Pane>
            <Pane maxWidth="1567.0" prefHeight="112.0" prefWidth="1366.0" style="-fx-border-color: #ccc; -fx-max-width: 1366;">
               <children>
                  <Button layoutX="1252.0" layoutY="23.0" mnemonicParsing="false" onAction="#voltar" prefHeight="80.0" prefWidth="100.0" text="Voltar">
                     <font>
                        <Font size="24.0" />
                     </font>
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
                  <Button alignment="CENTER" contentDisplay="CENTER" layoutX="16.0" layoutY="20.0" mnemonicParsing="false" onAction="#cadastrarUsuario" prefHeight="80.0" prefWidth="100.0" text="Novo Usuário" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font>
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
                  <Button layoutX="349.0" layoutY="20.0" mnemonicParsing="false" onAction="#novoSilo" prefHeight="80.0" prefWidth="100.0" text="Entrada de produtos" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font>
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
                  <Button cancelButton="true" layoutX="127.0" layoutY="20.0" mnemonicParsing="false" onAction="#cadastrarProdutor" prefHeight="80.0" prefWidth="100.0" text="Novo Produtor" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font>
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
                  <Button layoutX="239.0" layoutY="20.0" mnemonicParsing="false" onAction="#saidaDeProdutos" prefHeight="80.0" prefWidth="100.0" text="Saída de Produtos" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font>
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
                  <Button layoutX="460.0" layoutY="20.0" mnemonicParsing="false" onAction="#aReceber" prefHeight="80.0" prefWidth="100.0" stylesheets="@../styles/Style.css" text="A Receber" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="Times New Roman" size="16.0" />
                     </font>
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
               </children>
            </Pane>
      </children>
    </AnchorPane>
  </children>
</VBox>
